{
  
  "JavaScript": {
    "extensions": ["js", "json"],
    "name": {
      "hljs": "JavaScript"
    },
    "single": [
      "//"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "SCSS": {
    "extensions": ["scss"],
    "name": {
      "hljs": "SCSS"
    },
    "single": [
      "//"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "CSS": {
    "extensions": ["css"],
    "name": {
      "hljs": "CSS"
    },
    "single": [
      "//"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "C": {
    "extensions": ["c"],
    "name": {
      "hljs": "C"
    },
    "single": [
      "//"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "CPP": {
    "extensions": ["cpp", "cxx", "C"],
    "name": {
      "hljs": "CPP"
    },
    "single": [
      "//"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "Markdown": {
    "extensions": ["md"],
    "name": {
      "hljs": null
    }
  },
  
  "ARM Assembly": {
    "extensions": ["s", "S"],
    "name": {
      "hljs": "armasm",
      "color": "Assembly"
    },
    "single": [
      ";",
      "//",
      "@",
      "#"
    ],
    "multi": [
      ["/*", "*", "*/"]
    ]
  },
  
  "HTML": {
    "extensions": ["htm", "html"],
    "name": {
      "hljs": "html"
    },
    "multi": [
      ["<!--", "", "-->"]
    ]
  }
  
}

